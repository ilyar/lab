{
  "$schema": "../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/asset/manifest.webmanifest",
          "/*.json",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "asset",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/asset/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
